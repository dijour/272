<div class="row">
  <div class="col s7">
    <%= render :partial => "camp_details" %>
  </div>
  <div class="col s5">
    <% if can? :read, Instructor %>
      <%= render :partial => "camp_instructors_vue"%>
    <% end %> 
    <% if can? :show, Student %>
      <%= render :partial => "camp_students_vue"%>
    <% end %> 
    <% if current_user.nil? %> 
      <%= render :partial => "camp_spots"%>
    <% end %> 
    <%# if can? :create, Instructor %>
      <!--<button v-on:click="switch_modal()">Add Chore</button>-->
    <%# end %>
  </div>
</div>
<div class="row">
  <div class="col s12">
    <%= render :partial => "partials/object_options", locals: {model_name: "camp", object: @camp} %>
  </div>
</div>

