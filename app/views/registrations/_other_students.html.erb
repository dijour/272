<div class="card">
  <div class="card-content">
    <span class="card-title">Other Students</span>
    <% if @other_students.empty? %>
      <p>No other students registered at this time</p>
    <% else %>
      <ul>
        <% for student in @other_students %>
          <li style="materialize-red-text"><%= link_to student.proper_name, student_path(student) %></li>
        <% end %>
      </ul>
    <% end %>
  </div>
</div>